<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <TargetFramework>net7.0-android</TargetFramework>
    <IsBindingProject>true</IsBindingProject>
    <SupportedOSPlatformVersion>21</SupportedOSPlatformVersion>
    <Nullable>enable</Nullable>
    <ImplicitUsings>enable</ImplicitUsings>
    <RootNamespace>com.mapbox.mapboxsdk.mapbox_sdk_turf</RootNamespace>
    <AssemblyName>com.mapbox.mapboxsdk.mapbox_sdk_turf</AssemblyName>
    <AndroidClassParser>class-parse</AndroidClassParser>
    <GenerateAssemblyInfo>false</GenerateAssemblyInfo>
  </PropertyGroup>

    <ItemGroup>
        <AndroidLibrary Include="Jars\mapbox-sdk-turf-5.3.0.jar" />
    </ItemGroup>

    <ItemGroup>
      <PackageReference Include="GoogleGson" Version="2.10.1.5" />
    </ItemGroup>

    <ItemGroup>
      <ProjectReference Include="..\com.mapbox.mapboxsdk.mapbox-sdk-geojson\com.mapbox.mapboxsdk.mapbox-sdk-geojson.csproj" />
    </ItemGroup>
</Project>